<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.codet.back.user.mapper.UserMapper" >

	<select id="selectUserByUsername" parameterType="com.codet.pojo.User" resultType="com.codet.pojo.User">
		select * from User where username =#{username}
	</select>
	
	<select id="selectCustomAll" resultType="com.codet.pojo.Custom">
		select * from Custom 
	</select>
</mapper>